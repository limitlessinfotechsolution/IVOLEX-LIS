{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "domains": [
    {
      "domain": "ivolex.vercel.com"
    },
    {
      "domain": "ivolex.com"
    }
  ]
}
